Pattern 
I/P=3
O/P= * * *

     * * *

     * * * 


#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=1;i<=((2*n)-1);i++)
    {
        if(i%2!=0)
        {
           for(int j=1;j<=n;j++)
           {
                cout<<"* ";
           }
            cout<<endl; 
        }
        else
        {
            cout<<" "<<endl;
        }
    }
    return 0;
}
