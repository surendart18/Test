Count 'A' in the given string
I/P=AAABcde
O/P=3


#include<bits/stdc++.h>
using namespace std;
int main()
{
    string str;
    getline(cin,str);
    int count=0;
    for(int i=0;i<str.length();i++)
    {
       if(str[i]=='A')
       {
           count++;
       }
    }
    cout<<count;
    return 0;
}
